{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "PivotToLake",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Wait1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "part*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "drop table if exists datafactory.byCategorySummery",
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "GenericCSVFile",
						"type": "DatasetReference",
						"parameters": {
							"fileSystem": "stage",
							"pathToDir": "pivotDataPerPeriod"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "GenericSqlTable",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "datafactory",
							"TableName": "byCategorySummery",
							"LoadTime": {
								"value": "@formatDateTime(utcnow())",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "TotalSummery",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Wait1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "part*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "drop table if exists datafactory.totalSummery",
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "GenericCSVFile",
						"type": "DatasetReference",
						"parameters": {
							"fileSystem": "stage",
							"pathToDir": "totalSumerize"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "GenericSqlTable",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "datafactory",
							"TableName": "totalSummery",
							"LoadTime": "@formatDateTime(utcnow())"
						}
					}
				]
			},
			{
				"name": "Wait1",
				"type": "Wait",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"waitTimeInSeconds": 1
				}
			}
		],
		"annotations": []
	}
}