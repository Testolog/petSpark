{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "TestDataManipulationn",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.01:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/testolog5@gmail.com/TestDataManipulation",
					"baseParameters": {
						"inputContainer": "raw",
						"outputContainer": "stage",
						"storageName": "gen2petstorage"
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricksFactory",
					"type": "LinkedServiceReference",
					"parameters": {
						"containerName": "raw"
					}
				}
			},
			{
				"name": "PivotToLake",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "TestDataManipulationn",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "GenericCSVFile",
						"type": "DatasetReference",
						"parameters": {
							"fileSystem": "stage",
							"pathToDir": "pivotDataPerPeriod"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "GenericSqlTable",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "datafactory",
							"TableName": "pivotData",
							"LoadTime": {
								"value": "@formatDateTime(utcnow())",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "TotalSummery",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "TestDataManipulationn",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "part*",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "totalSumerize",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "GenericSqlTable",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "datafactory",
							"TableName": "totalSummery",
							"LoadTime": "@formatDateTime(utcnow())"
						}
					}
				]
			}
		],
		"annotations": []
	}
}